<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <IncludePath>$(VC_IncludePath);$(SolutionDir)\external\easy_profiler\prof_debug\include;$(WindowsSDK_IncludePath);$(SolutionDir)</IncludePath>
    <LibraryPath>$(SolutionDir)\external\easy_profiler\prof_debug\bin;$(LibraryPath)</LibraryPath>
    <CustomBuildBeforeTargets>ClCompile</CustomBuildBeforeTargets>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <PrecompiledHeaderOutputFile>$(EnginePch)</PrecompiledHeaderOutputFile>
      <LanguageStandard>stdcpp17</LanguageStandard>
    </ClCompile>
    <CustomBuildStep>
      <Command>if EXIST "$(EnginePdb)" xcopy /Y /F "$(EnginePdb)" "$(IntDir)"
if EXIST "$(EngineIdb)" xcopy /Y /F "$(EngineIdb)" "$(IntDir)"</Command>
      <Inputs>$(EnginePdb)</Inputs>
      <Outputs>$(IntDir)vc$(PlatformToolsetVersion).pdb;</Outputs>
    </CustomBuildStep>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>